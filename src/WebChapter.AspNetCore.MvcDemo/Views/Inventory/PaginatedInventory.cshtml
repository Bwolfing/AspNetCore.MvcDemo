@model IPaginatedViewModel<ItemViewModel>

<table class="ui celled table">
    <thead>
        <tr>
            <th>
                Item Name
            </th>
            <th>
                Product SKU
            </th>
            <th>
                Supply
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Models)
        {
            <tr>
                <td>
                    @item.Name
                </td>
                <td>
                    @item.Sku
                </td>
                <td>
                    @item.Supply
                </td>
            </tr>
        }
    </tbody>
    <tfoot>
        <tr>
            <th colspan="3">
                <demo-pager model="Model" />
            </th>
        </tr>
    </tfoot>
</table>